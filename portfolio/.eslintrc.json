{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb",
        "airbnb-typescript"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": ["./profile/tsconfig.json"]
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/function-component-definition": [
            "error",
            {
              "namedComponents": ["function-declaration", "arrow-function"],
              "unnamedComponents": "arrow-function"
            }
          ]
    }
}
